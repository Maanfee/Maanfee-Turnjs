@using static Maanfee.Turnjs.FlipBook

<MudDialog>
    <DialogContent>

        <style type="text/css">

            .mud-dialog-content {
                overflow: hidden !important;
            }

        </style>

        <MudFab Style="z-index: 1000;position: absolute;" OnClick="Cancel" Color="Color.Secondary" StartIcon="@Icons.Material.Filled.Close" Size="Size.Small" />

        <FlipBook @ref=FlipBook Pages="@pages" Options="@options" PageChanged="OnPageChanged" />

        @*  <div style="z-index: 1000;position: absolute;text-align:center">
            @Log
        </div> *@

    </DialogContent>
</MudDialog>

@code {

    [CascadingParameter]
    private IMudDialogInstance MudDialog { get; set; }

    private void Cancel() => MudDialog.Cancel();

    // *****************************************************

    private FlipBook FlipBook = new FlipBook();
    private string Log = string.Empty;
    private List<TurnjsPage> pages = new();
    private TurnjsOptions options = new()
    {
        AutoCenter = true,
        Direction = TurnjsDirection.RTL,
    };

    protected override void OnInitialized()
    {
        string IsRtl = options.Direction == TurnjsDirection.RTL ? "rtl" : "ltr";

        pages.Add(new TurnjsPage
        {
            PageToolbar = false,
            PageTitle = "Turn.js",
            PageType = PageType.FrontCover,
            CssClass = "hard",
        });

        pages.Add(new TurnjsPage
        {
            PageToolbar = false,
            PageType = PageType.FrontCover_Back,
            CssClass = "hard",
            IsPageMirror = options.Direction == TurnjsDirection.RTL ? false : true,
        });

        // Table Contents
        pages.Add(new TurnjsPage
        {
            PageToolbar = false,
            ContentTemplate = (page) => @<div class="@($"table-contents {IsRtl}")">
                <h1>Contents</h1>
                <ul>
                    <li><a onclick="@(() => OnPageClicked(4))">Load Image Async<span>4</span></a></li>
                    <li><a onclick="@(() => OnPageClicked(9))">Image Page<span>9</span></a></li>
                    <li><a onclick="@(() => OnPageClicked(10))">Page Foreach<span>10</span></a></li>
                    <li><a onclick="@(() => OnPageClicked(15))">Custom Page<span>15</span></a></li>
                    <li><a onclick="@(() => OnPageClicked(pages.Count - 2))">The End<span>@(pages.Count - 2)</span></a></li>
                    <li><a href="#page/38">Macintosh <span>38</span></a></li>
                    <li><a href="#page/40">Leaving Apple  <span>40</span></a></li>
                    <li><a href="#page/43">NeXT Inc <span>43</span></a></li>
                    <li><a href="#page/53">Wildreness years <span>53</span></a></li>
                    <li><a href="#page/73">A visionary <span>73</span></a></li>
                    <li><a href="#page/86">Apple is strong <span>86</span></a></li>
                </ul>
            </div>,
        });

        // Load Content Async
        pages.Add(new TurnjsPage
        {
            IsLoaded = false,
            LoadContentAsync = async (page) =>
               {
                   // شبیه‌سازی بارگذاری غیرهمزمان
                   await Task.Delay(2000);

                   page.TitleTemplate = (page) =>@<div class="page-title">Load Content Async</div>;
                   page.ContentTemplate = (page) =>@<img class='zoom-this'
                        src='data:image/jpeg;base64,' />
                   ;
               },
        });

        // Load Content Async
        pages.Add(new TurnjsPage
        {
            IsLoaded = false,
            LoadContentAsync = async (page) =>
               {
                   // شبیه‌سازی بارگذاری غیرهمزمان
                   await Task.Delay(2000);

                   page.TitleTemplate = (page) =>@<div class="page-title">Load Content Async</div>;
                   page.ContentTemplate = (page) =>@<img class='zoom-this'
                        src='data:image/jpeg;base64,' />
                   ;
               },
        });

        // Load Content Async
        pages.Add(new TurnjsPage
        {
            IsLoaded = false,
            LoadContentAsync = async (page) =>
               {
                   // شبیه‌سازی بارگذاری غیرهمزمان
                   await Task.Delay(2000);

                   page.TitleTemplate = (page) =>@<div class="page-title">Load Content Async</div>;
                   page.ContentTemplate = (page) =>@<img class='zoom-this'
                        src='data:image/jpeg;base64,' />
                   ;
               },
        });

        // Load Content Async
        pages.Add(new TurnjsPage
        {
            IsLoaded = false,
            LoadContentAsync = async (page) =>
               {
                   // شبیه‌سازی بارگذاری غیرهمزمان
                   await Task.Delay(2000);

                   page.TitleTemplate = (page) =>@<div class="page-title">Load Content Async</div>;
                   page.ContentTemplate = (page) =>@<img class='zoom-this'
                        src='data:image/jpeg;base64,' />
                   ;
               },
        });

        // Load Content Async
        pages.Add(new TurnjsPage
        {
            IsLoaded = false,
            LoadContentAsync = async (page) =>
               {
                   // شبیه‌سازی بارگذاری غیرهمزمان
                   await Task.Delay(2000);

                   page.TitleTemplate = (page) =>@<div class="page-title">Load Content Async</div>;
                   page.ContentTemplate = (page) =>@<img class='zoom-this'
                        src='data:image/webp;base64,' />
                   ;
               },
        });

        // یا با تمپلیت سفارشی
        pages.Add(new TurnjsPage
        {
            TitleTemplate = (page) => @<div class="page-title">Sample Image</div>,
            ContentTemplate = (page) => @<img class='zoom-this'
                 src='data:image/jpeg;base64,' />
        });

        // نمونه صفحات با محتوای ساده
        for (int i = 1; i <= 5; i++)
        {
            pages.Add(new TurnjsPage
            {
                PageTitle = $"{i}",
                PageContent = $"محتوای صفحه {i}",
                PageFooter = $"محتوای",
            });
        }

        // یا با تمپلیت سفارشی
        pages.Add(new TurnjsPage
        {
            ContentTemplate = (page) => @<div>
                <h3>صفحه سفارشی</h3>
                <p>شما می‌توانید هر محتوایی اینجا قرار دهید</p>
                <p>@pages.Count()</p>
            </div>
        });

        if (pages.Count() % 2 != 0)
        {
            pages.Add(new TurnjsPage());
        }

        pages.Add(new TurnjsPage
        {
            PageToolbar = false,
            PageType = PageType.BackCover_Back,
            CssClass = "hard",
            IsPageMirror = options.Direction == TurnjsDirection.RTL ? false : true,
        });

        pages.Add(new TurnjsPage
        {
            PageToolbar = false,
            PageType = PageType.BackCover,
            CssClass = "hard",
        });

        <!-- Next button -->
        pages.Add(new TurnjsPage { CssClass = "next-button", IgnorePage = true, PageToolbar = false });
        <!-- Previous button -->
        pages.Add(new TurnjsPage { CssClass = "previous-button", IgnorePage = true, PageToolbar = false });
    }

    private async Task OnPageClicked(int PageNumber)
    {
        await Turnjs.GoToPage(PageNumber);
    }

    private void OnPageChanged(int pageNumber)
    {
        Log = ($"صفحه فعلی: {pageNumber}");
    }

}
